                     <msItem xml:id="MS_Bodl_177-part3-item1" n="1" class="#medica">
                        <title type="desc" key="work_13232">Medical
                           treatises</title>
                        <msItem>
                           <locus>(fol. 33)</locus>
                           <rubric>Tracatus vrinarum</rubric>
                           <incipit>In tractatu vrinarum primum quid vrina sit videndum</incipit>
                        </msItem>
                        <msItem>
                           <locus>(fol. 34v)</locus>
                           <rubric>Liber pulsuum</rubric>
                        </msItem>
                        <msItem>
                           <locus>(fol. 38)</locus>
                           <rubric>Modus iudicandi urinas</rubric>
                           <note>‘Secundum galterum’.</note>
                        </msItem>
                        <msItem>
                           <locus>(fol. 38v)</locus>
                           <rubric>Liber vrinarum Egidij cum commento</rubric>
                           <note>The text of the poem is not given.</note>
                        </msItem>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem xml:id="MS_Bodl_177-part3-item2" n="2">
                        <title type="desc" key="work_14987">Treatise on chiromancy</title>
                        <incipit>Si linea vite sit grossa</incipit>
                        <incipit>Ad sciendum artem chiromanticam</incipit>
                        <note resp="#MLH">Cf. TK 60J, 1108J, 1453K (?)</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem n="3" xml:id="MS_Bodl_177-part3-item3">
                        <locus>(fol. 42)</locus>
                        <title type="desc" key="work_10342">Astrological treatise</title>
                        <rubric>Fortuna de xii signis</rubric>
                        <incipit>Qui fuerit natus in signo Aquarij</incipit>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem xml:id="MS_Bodl_177-part3-item4" n="4" class="#mathematica">
                        <locus>(fol. 45)</locus>
                        <author key="person_84975906">Johannes de Sacro Bosco</author>
                        <title key="work_2590">Algorismus</title>
                        <note>Ends imperfect in ch. 4 (here ch. 5). Each chapter followed by the corresponding passage of <persName role="aut" key="person_2583890">Alexander de Villa Dei</persName>, <title key="work_409">Algorismus</title>.</note>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                     <msItem xml:id="MS_Bodl_177-part3-item5" n="5" class="#astrologia_astronomia #prognostica">
                        <locus>(fol. 46v)</locus>
                        <title type="desc" key="work_14987">Treatise on chiromancy</title>
                        <incipit>Chiromancia est sciencia cognoscendi</incipit>
                        <textLang mainLang="la">Latin</textLang>
                     </msItem>
                  </msContents>
