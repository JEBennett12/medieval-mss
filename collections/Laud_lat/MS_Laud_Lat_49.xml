                  <msContents>
                     <textLang mainLang="la">Latin</textLang>
                     <msItem xml:id="MS_Laud_Lat_49-part1-item1" n="1">
                        <locus>(fols. 1ra-3ra)</locus>
                        <author key="person_64016141">
                           Porphyry
                        </author>
                        <title key="work_3764">Isagoge</title><note>(tr. <persName role="trl" key="person_100218964">Boethius</persName>)</note>
                        <incipit>Cum sit necessarium crisarori</incipit>
                        <explicit>eorum communitatisque traditionem
                        </explicit></msItem>
                   
                     <msItem xml:id="MS_Laud_Lat_49-part1-item2" n="2">
                        <locus>(fols. 3ra-9vb, 10ra-14ra)</locus><!-- check -->
                        <author key="person_100218964">Boethius</author>
                        <title key="work_6426"> In Isagogen Porphyrii Commentorum editionis primae</title>
                        <rubric>Anicii Man<ex>lii</ex> S<ex>everini</ex> Boetii <gap reason="editorial"/> in Porphyrii Ysag<ex>ogen</ex> a Mario Victorino translatas editionis prime liber primus incipit <note type="editorial">[in capitals]</note></rubric>
                        <incipit>Hiemantis anni tempore</incipit>
                        <explicit>consideratione tractabitur</explicit>
                        <note>Bk. i ends fol. 9vb, followed by art. 3 below; bk. ii begins fol. 10ra.</note>
                     </msItem>
                   
                     <msItem xml:id="MS_Laud_Lat_49-part1-item3" n="3">
                        <locus>(fols. 9vb-c, 9v lower margin; 10r-v lower margin; 27vb-28vc)</locus>
                        <author key="person_96533523">Alcuin</author>
                        <title key="work_377">De dialectica</title>
                        <rubric>Ysag. Albini</rubric><!-- add fol. 27vb -->
                        <incipit>Quia mentionem philosophię</incipit>
                        <note>Ch. 3 begins fol. 27vb. Rest of fol. 28vc blank except for rubric for art. ... at bottom.</note>
                     </msItem>
                     
                     <msItem xml:id="MS_Laud_Lat_49-part1-item4" n="4">
                        <locus>(fols. 14ra-27vb)</locus>
                        <author key="person_100218964">Boethius</author>
                        <title key="work_6426"> In Isagogen Porphyrii Commentorum editionis secunda</title>
                        <rubric>Anicii Manlii Severini Boetii <gap reason="editorial"/> in Porphyrii Ysagogas a se translatas editionis secunde liber I incipit <note type="editorial">[in capitals]</note></rubric>
                        <incipit>Secundus his arreptę expositionis labor</incipit>
                        <explicit>ad predicamenta seruanti</explicit></msItem>
                   
                     <msItem><note>Fols. 27vb-28vc, see art. 3 above.</note></msItem>
                   
                     <msItem xml:id="MS_Laud_Lat_49-part1-item5" n="5">
                        <locus>(fols. 29ra-32va)</locus>
                        <author key="person_7524651">Aristotle</author>
                        <title key="work_6427">Categoriae</title><note>(tr. <persName role="trl" key="person_100218964">Boethius</persName>)</note>
                        <rubric><locus>(fol. 28vc)</locus> Incipivnt cathegoriae Aristotelis</rubric>
                        <incipit>AEQUIUOCA DICVNTVR <add>quorum nomen solum commune est secundum nomen vero substan</add>tię ratio diuersa</incipit>
                        <explicit>omnes annumerati</explicit>
                        <note>Fol. 32vb-c blank.</note>
                        </msItem>
                   
                     <msItem xml:id="MS_Laud_Lat_49-part1-item6" n="6">
                        <locus>(fols. 33ra-56va)</locus>
                        <author key="person_100218964">Boethius</author>
                        <title key="work_6428">In Categorias Aristotelis</title>
                        <rubric>Anicii Manlii Severini Boetii <gap reason="editorial"/> editionis primę in Aristotelis Cathegorias a se translatas incipit liber primvs</rubric>
                        <incipit>Expeditis hiis quę ad predicamenta</incipit>
                        <explicit>aliquas partes speciesque contineat</explicit></msItem>
                  
                     <msItem xml:id="MS_Laud_Lat_49-part1-item7" n="7">
                        <locus>(fols. 56va-58vc)</locus>  <author key="person_7524651">Aristotle</author>
                        <title key="work_650">De interpretatione</title><note>(tr. <persName role="trl" key="person_100218964">Boethius</persName>)</note>
                        <rubric><add>Textus libri periermen.</add></rubric>
                      
                        <incipit>Primum oportet constituere</incipit>
                        <explicit>non contingit inesse contraria</explicit></msItem>
                  
                     <msItem xml:id="MS_Laud_Lat_49-part1-item8" n="8">
                        <locus>(fol. 58vc-76rc)</locus>
                        <author key="person_100218964">Boethius</author>
                        <title key="work_6429">In librum Aristotelis Perihermenias commentarii editio prima</title>
                        <rubric>Anicii M. S. Boetii <gap reason="editorial"/> In πηριηρμηνιας editionis .i. liber .i. incipit</rubric>
                        <incipit>Magna quidem libri huius apud peripatheticam sectam probatur auctorias</incipit>
                        <explicit>editionis series expliabit</explicit>
                        <finalRubric><add place="margin">Explicit</add></finalRubric>
                     </msItem>
                     <msItem xml:id="MS_Laud_Lat_49-part1-item9" n="9">
                        <locus>(fols. 76rc-va)</locus>
                        <title><supplied>On the name and title of Boethius and the definition of philosophy</supplied></title>
                        <incipit>Anicius ut quidam putant ab anno dictus, eo quod optabant parentes eius eum annosum fieri</incipit>
                        <incipit><locus>(fol. 76va)</locus> Philosophia est amor sapientię et inquisitio uel cognitio diuinarium humanarumque rerum. Aliter, philosophia est honestas uitę, studium bene uiuendi</incipit>
                        <explicit>Aethica socrates primus ad componendos mores instituit <gap reason="editorial"/> Logicam que rationalis uocatur plato sub iunxit diuidens in dialecticam et rethoricam</explicit>
                        <note>Follows without a break from the preceding item. Rest of fol. 76va blank.</note>
                        </msItem>
                     
                     <msItem xml:id="MS_Laud_Lat_49-part1-item10" n="10">
                        <locus>(fols. 76vb-96vc)</locus>
                        <author key="person_100218964">Boethius</author>
                        <title key="work_6429">In librum Aristotelis Perihermenias commentarii editio secunda</title>
                        <rubric><add>Incipit secunda edicio boecij super librum peryermenias aristotelis</add></rubric>
                        <incipit>Alexander in commentariis suis</incipit>
                        <explicit defective="true"> illas quidem quas cathegoricas greci nominant</explicit>
                        <!-- TODO check lib iii intro -->
                        <note>Fol. 97r blank.</note>
                     </msItem>
                     <msItem xml:id="MS_Laud_Lat_49-part1-item11" n="11">
                        <locus>(fols. 97ra-100vc)</locus>
                        <author key="person_78769600">Cicero</author>
                        <title key="work_1274">Topica</title>
                        <rubric><add>Textus </add></rubric><!-- TODO  198 -->
                        <incipit>MAIORES NOS RES SRIBERE ingressos </incipit>
                        <explicit>non debita accedere</explicit>
                        <finalRubric><add>Explicit liber topicorum Marci tulii ad trebatium</add></finalRubric>
                        <!-- many glosses deleted? -->
                     </msItem>
                     <msItem xml:id="MS_Laud_Lat_49-part1-item12" n="12">
                        <locus>(fols. 101ra-128rc)</locus>
                        <author key="person_100218964">Boethius</author>
                        <title key="work_7323">In Ciceronis Topica commentum</title>
                        <rubric><add>Commentum <add>subsequens est</add> antecedentis immediate libri <add>a Boetio editum</add></add></rubric>
                        <incipit>Exhortatione mi patri cum rethoricum</incipit><!-- check 205 -->
                        <explicit>ad culmen perfection<subst><del>e</del><add>i</add></subst> adducitur</explicit>
                        <note>Rest of fol. 128rc blank; fol. 128v blank.</note>
                     </msItem>
                     <msItem xml:id="MS_Laud_Lat_49-part1-item13" n="13">
                       <msItem><locus>(fols. 129ra-146rc)</locus>
                        <author key="person_78769600">Cicero</author>
                        <title key="work_1244">De inuentione</title>
                       <incipit><locus>(fol. 137va)</locus> Sępe et multum hoc mecum cogitaui</incipit>
                       <incipit><locus>(fol. 129ra)</locus> inopia infirmitas solitudo</incipit>  <explicit><locus>(fol. 146vc)</locus> uel laudator dicitur</explicit></msItem>
                     
                        <msItem >
                          <locus>(fols. 129ra-146rc)</locus>
                          <incipit><locus>(fol. 137va, margin)</locus> Primum in hoc libro notandum quod quicumque incipit orationem tria adhibere debet</incipit>
                         <!-- <explicit><locus>(fol. 136vv, margin)</locus> in parentum manum bona sua transirent</explicit>--> <!-- 295 fol. 146) -->
                       </msItem>
                       <note>Misbound: fols. 129-136 should follow fols. 137-145. Fol. 137r blank.</note></msItem>
                     <msItem xml:id="MS_Laud_Lat_49-part1-item14" n="14">
                   <locus>(fol. 146va-166rc)</locus>
                        <author key="person_89512766">Marius Victorinus</author>
                        <title key="work_4908"> Explanationes in Ciceronis Rhetoricam </title>
                        <incipit>OMNIS QUVICVQUE incipit cuiuscumque generis orationem hec tria in principus adhibere debet</incipit>
                        <explicit>alias quod per affectionem</explicit>
                        <note>Breaks off unfinished.</note>
                        <!-- https://archive.org/details/rhetoreslatinim00halmgoog/page/n254/mode/1up check ending -->
                     </msItem>
                   
                 
